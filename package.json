{
  "author": "Jim Razmus II",
  "auto-changelog": {
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "bin": {
    "aam": "./bin/cli.js"
  },
  "bugs": {
    "url": "https://github.com/jimrazmus/ami-access-manager/issues"
  },
  "dependencies": {
    "ajv": "6.9.2",
    "aws-sdk": "2.420.0",
    "commander": "2.19.0",
    "convict": "4.4.1",
    "dotenv": "6.2.0",
    "loglevel": "1.6.1",
    "p-queue": "4.0.0"
  },
  "description": "Manage AMI launch permissions",
  "devDependencies": {
    "auto-changelog": "1.11.0",
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "jest": "24.5.0",
    "jest-junit": "6.3.0",
    "prettier": "1.16.4"
  },
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=10.15.1"
  },
  "homepage": "https://github.com/jimrazmus/ami-access-manager",
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./reports/jest/"
  },
  "keywords": [
    "amazon",
    "aws",
    "ami"
  ],
  "license": "MIT",
  "name": "ami-access-manager",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jimrazmus/ami-access-manager"
  },
  "scripts": {
    "clean": "rm -rf ./{coverage,reports}",
    "lint": "eslint --format junit --output-file ./reports/eslint/eslint.xml 'src/**/*.js'",
    "postversion": "git push && git push --tags",
    "pretty": "prettier --write \"{bin,src}/**/*.js\"",
    "pretty-test": "prettier --list-different \"{bin,src}/**/*.js\"",
    "test": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "version": "0.3.0"
}
